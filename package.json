{
  "name": "eightgent",
  "version": "1.0.0",
  "description": "Agentic code generation platform - transforms PRDs into executable code using AI",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:tools && npm run build:generated",
    "build:tools": "cd tools && npm run build",
    "build:generated": "tsc",
    "clean": "rm -rf dist && cd tools && npm run clean",
    "dev:tools": "cd tools && npm run dev",
    "test-local": "cd tools && npm run test-local",
    "process-prd": "cd tools && pnpm run process-prd --",
    "process-prd-openai": "cd tools && pnpm run process-prd-openai --",
    "process-prd-smart": "cd tools && pnpm run process-prd-smart --"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.5.3"
  },
  "private": true
}
